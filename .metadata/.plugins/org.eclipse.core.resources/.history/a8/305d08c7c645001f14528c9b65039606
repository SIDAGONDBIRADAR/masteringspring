package com.eazybytes.eazyschool.model;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;
import lombok.Data;

@Data
@Entity
@Table(name = "holidays")
public class Holiday extends BaseEntity{

    private  String day;
    private  String reason;
    private  Type type;

    public enum Type {
        FESTIVAL, FEDERAL
    }
}
